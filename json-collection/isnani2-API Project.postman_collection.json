		{
			"name": "Single Book",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Updated the book details and added a more descriptive test name\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Response time is less than 500ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(500);\r",
							"});\r",
							"pm.test(\"Verify Book Details and Match Expected Values\", () => {\r",
							"    let jsonData = pm.response.json();\r",
							"\r",
							"    const expectedValues = {\r",
							"        id: 1,\r",
							"        name: \"The Russian\",\r",
							"        author: \"James Patterson and James O. Born\",\r",
							"        isbn: \"1780899475\",\r",
							"        type: \"fiction\",\r",
							"        price: 12.98,\r",
							"        \"current-stock\": 12,\r",
							"        available: true\r",
							"    };\r",
							"\r",
							"    for (const key in expectedValues) {\r",
							"        pm.expect(jsonData[key]).to.eql(expectedValues[key]);\r",
							"    }\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
